{
  "name": "poe-log-events",
  "version": "3.2.0",
  "description": "Monitors the PoE log file and emits events",
  "scripts": {
    "build": "tsc && npm run cpy-resources",
    "test": "dotenv -e .env nyc mocha",
    "test:ci": "nyc mocha",
    "lint": "eslint src/ tests/",
    "prettier": "prettier --write \"**/*.ts\"",
    "semantic-release": "semantic-release",
    "cpy-resources": "ncp src/resources dist/resources",
    "updateCurrencies": "ts-node scripts/updateCurrencies.ts"
  },
  "keywords": [
    "Typescript",
    "Path of Exile",
    "poe",
    "log",
    "eventEmitter"
  ],
  "author": {
    "name": "Andreas Schmidt",
    "email": "moepmoep12@gmail.com"
  },
  "license": "MIT",
  "repository": "git://github.com/moepmoep12/poe-log-events.git",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/chai": "^4.3.1",
    "@types/debug": "^4.1.7",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.33",
    "@types/prettier": "^2.7.1",
    "@types/tail": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^10.0.0",
    "mocha-steps": "^1.3.0",
    "ncp": "^2.0.0",
    "nyc": "^15.1.0",
    "poe-api-ts": "^2.1.0",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.3",
    "source-map-support": "^0.5.21",
    "ts-morph": "^16.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "main": "./dist/index.js",
  "files": [
    "./dist"
  ],
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/changelog",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "message": "Release <%= nextRelease.version %> [skip ci]",
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "debug": "^4.3.4",
    "tail": "^2.2.4",
    "tiny-typed-emitter": "^2.1.0"
  }
}
